{
  "name": "oncehub-ui",
  "version": "4.0.2-beta.2",
  "scripts": {
    "ng": "ng",
    "build": "ng build ui",
    "postbuild": "gulp copy",
    "test": "ng test ui",
    "lint": "eslint ui --ext .ts ui",
    "prettier": "prettier --list-different \"**/*.{ts,js,css,scss,md,html}\"",
    "e2e": "ng e2e",
    "fix:prettier": "prettier --write \"**/*.{ts,js,css,scss,md,html}\"",
    "fix:lint": "eslint --fix --ext .ts ui",
    "bundle-styles": "npm run bundle-styles-blue && npm run compile-scss-blue && npm run bundle-styles-green && npm run compile-scss-green",
    "bundle-styles-blue": "scss-bundle -e ./ui/src/components/core/themes/once-ui-theme-blue.scss -o ./dist/ui/themes/once-ui-theme-blue.scss",
    "bundle-styles-green": "scss-bundle -e ./ui/src/components/core/themes/once-ui-theme-green.scss -o ./dist/ui/themes/once-ui-theme-green.scss",
    "compile-scss-blue": "node-sass --include-path scss ./dist/ui/themes/once-ui-theme-blue.scss ./dist/ui/themes/once-ui-theme-blue.css && rimraf ./dist/ui/themes/once-ui-theme-blue.scss",
    "compile-scss-green": "node-sass --include-path scss ./dist/ui/themes/once-ui-theme-green.scss ./dist/ui/themes/once-ui-theme-green.css && rimraf -f ./dist/ui/themes/once-ui-theme-green.scss",
    "npm-pack": "cd dist/ui && npm pack",
    "package": "npm run build && npm run bundle-styles && npm run npm-pack",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "build-storybook": "npm run storybook-css && npm run docs:json &&  build-storybook -s ./ui/src/stories/styles",
    "storybook-css": "node-sass --output-style compressed  ui/src/components/core/theming/prebuilt -o ui/src/stories/styles/themes",
    "storybook": "npm run storybook-css && npm run docs:json && start-storybook -p 6006 -s ./ui/src/stories/styles",
    "deploy-storybook": "npm run storybook-css && storybook-to-ghpages -o out"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^11.1.2",
    "@angular/animations": "^11.1.1",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "^11.1.1",
    "@angular/compiler": "^11.1.1",
    "@angular/core": "^11.1.1",
    "@angular/forms": "^11.1.1",
    "@angular/platform-browser": "^11.1.1",
    "@angular/platform-browser-dynamic": "^11.1.1",
    "@angular/router": "^11.1.1",
    "core-js": "^3.8.3",
    "rxjs": "^6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.2",
    "@angular/cli": "^11.1.2",
    "@angular/compiler-cli": "^11.1.1",
    "@angular/language-service": "^11.1.1",
    "@babel/core": "^7.12.10",
    "@compodoc/compodoc": "^1.1.11",
    "@oncehub/eslint-config": "^1.1.1",
    "@storybook/addon-actions": "6.1.15",
    "@storybook/addon-docs": "6.1.15",
    "@storybook/addon-essentials": "6.1.15",
    "@storybook/addon-knobs": "6.1.15",
    "@storybook/addon-links": "6.1.15",
    "@storybook/angular": "6.1.15",
    "@storybook/storybook-deployer": "^2.8.7",
    "@types/jasmine": "^3.6.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.22",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.1",
    "eslint": "^7.19.0",
    "gulp": "^4.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.0.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^11.1.2",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "puppeteer": "5.5.0",
    "rimraf": "^3.0.2",
    "scss-bundle": "^3.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
